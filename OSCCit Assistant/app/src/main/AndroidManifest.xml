<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="osccitassistant">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OSCCitAssistant">
        <activity
            android:name="com.james.osccitassistant.ICAnalysisResultsActivity"
            android:exported="true" />
        <activity
            android:name="com.james.osccitassistant.CommenceICAnalysisActivity"
            android:exported="true" />
        <activity android:name="com.james.osccitassistant.IWAnalysisResultsActivity" />
        <activity android:name="com.james.osccitassistant.NewCalibrationParameterActivity" />
        <activity android:name="com.james.osccitassistant.CommenceIWAnalysisActivity" />
        <activity android:name="com.james.osccitassistant.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>